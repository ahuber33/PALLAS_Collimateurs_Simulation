{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug PALLAS_CollSim",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/PALLAS_CollSim",
      "args": ["vis"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/bin",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty printing",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
